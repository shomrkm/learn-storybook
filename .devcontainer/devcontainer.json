{
  "name": "learn-storybook",
  "dockerComposeFile": ["../docker-compose.yml"],
  "customizations": {
    "vscode": {
      "settings": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
          "source.fixAll.eslint": true
        },
      },
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
      ],
    },
  },
  "service": "app",
  "workspaceFolder": "/app",
  "shutdownAction": "stopCompose"
}
